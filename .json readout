import json

def categorize_data(data, threshold):
    categorized_data = []

    for value in data:
        if value > threshold:
            categorized_data.append("hit")
        else:
            categorized_data.append("no-hit")

    return categorized_data

def main():
    # Read data from the .json file
    with open("data.json", "r") as json_file:
        json_data = json.load(json_file)

    data_values = json_data["data"]

    threshold = 500

    # Categorize the data based on the threshold
    categorized_data = categorize_data(data_values, threshold)

    # Print the results
    for index, value in enumerate(data_values):
        print(f"Data point {index + 1}: {value} -> {categorized_data[index]}")

if __name__ == "__main__":
    main()
