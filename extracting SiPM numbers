def extract_sipm_numbers(file_path):
    with open(file_path, 'r') as file:
        for line in file:
            # Skip header lines
            if line.startswith('###'):
                continue

            # Split the line by whitespace
            columns = line.split()

            # Check if the line contains enough columns (SiPM number is in the 5th column)
            if len(columns) >= 5:
                sipm_number = columns[4]
                print(sipm_number)


if __name__ == "__main__":
    text_file_path = "path/to/your/textfile.txt"  # Replace with the actual path to your text file
    extract_sipm_numbers(text_file_path)
